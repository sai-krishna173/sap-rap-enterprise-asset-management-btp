projection;
strict ( 2 );
use draft;

define behavior for ZC_EALM_ASSET alias Asset
{
  use create;
  use update;
  use delete;

  use action Activate;
  use action Discard;
  use action Resume;
  use action Edit;
  use action Prepare;

  use association _Maintenance { create; with draft; }
}

define behavior for ZC_EALM_MAINTENANCE alias Maintenance
{
  use update;
  use delete;

  use association _Asset { with draft; }
}